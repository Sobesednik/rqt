{
  "presets": [
    [
      "@babel/env",
      {
        "exclude": [
          "transform-regenerator",
          "transform-async-to-generator"
        ],
        "modules": false,
        "debug": true,
        "targets": {
          "node": 4
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    [
      "module:fast-async",
      {
        "spec": true
      }
    ],
    [
      "transform-rename-import",
      {
        "original": "../../package.json",
        "replacement": "../../../package.json"
      },
      "replace-package-1"
    ],
    [
      "transform-rename-import",
      {
        "original": "../package.json",
        "replacement": "../../package.json"
      },
      "replace-package-2"
    ],
    [
      "transform-rename-import",
      {
        "original": "erotic",
        "replacement": "erotic/es5/src"
      },
      "replace-erotic"
    ]
  ]
}
