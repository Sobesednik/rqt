  },
  "homepage": "https://github.com/artdecocode/rqt#readme",
  "devDependencies": {
    "documentary": "1.6.1",
    "eslint-config-artdeco": "1.0.0",
    "https-context": "1.0.1",
    "idio": "2.7.0",
    "zoroaster": "2.1.0"
  },
  "dependencies": {
    "catchment": "2.0.1",
    "erotic": "1.2.1"
