{
  "name": "rqt",
  "version": "3.1.4",
  "description": "Easy-To-Use Request Library That Supports String, JSON And Buffer Requests, Timeouts, GZip Compression And Session Maintenance.",
  "main": "build",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _rqt",
    "compile": "depack-dev src/depack.js -o compile/rqt.js -c -a -p -s --source_map_include_content",
    "d1": "typal types/index.js",
    "externs": "typal types/externs.js",
    "d": "yarn-s d1 externs",
    "lint": "eslint .",
    "b": "alamode src -o build -s",
    "build": "yarn-s d b doc",
    "example/rqt.js": "node example example/rqt.js",
    "e": "alanode"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/rqt.git"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "request",
    "network",
    "session",
    "json",
    "form",
    "post",
    "get",
    "cookies",
    "http",
    "https"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/rqt/issues"
  },
  "homepage": "https://rqt.biz",
  "devDependencies": {
    "@idio/idio": "^0.1.0",
    "@wrote/read": "^1.0.4",
    "alamode": "^2.3.6",
    "documentary": "^1.29.0",
    "typal": "^1.19.0",
    "eslint-config-artdeco": "1.0.1",
    "https-context": "^1.0.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "@rqt/aqt": "^1.3.0"
  }
}
